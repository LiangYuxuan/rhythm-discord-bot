    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pino": "^6.11.3",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/global-agent": "^2.1.1",
    "@types/node": "^15.0.1",
