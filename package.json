  "main": "app.js",
  "scripts": {
    "build": "tsc --project ./",
    "start": "nodemon --watch 'src/' -e ts --exec 'ts-node' ./src/app.ts",
    "start:no-discord": "NO_DISCORD=1 nodemon --watch 'src/' -e ts --exec 'ts-node' ./src/app.ts",
    "dev": "NODE_ENV=development nodemon --watch 'src/' -e ts --exec 'ts-node' ./src/app.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
