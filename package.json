    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pino": "^7.5.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/global-agent": "^2.1.1",
